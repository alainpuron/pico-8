function asteroid_hit_plr()

	local hit_plr,a = collided(asteroids,plr,10)
	
		if hit_plr then
			destroy(asteroids,a)
			lose_health(plr)
		end
		
end

function alien_hit_plr()

		local hit_alien,s = collided(alien,plr,10)
		
		if hit_alien then
				destroy(alien,s)
				lose_health(plr)
		end
		
end


function bullet_hit_plr()

		local hit_alien_bullet,asb = collided(ab,plr,10)
		
		if hit_alien_bullet then
		
				lose_health(plr)
				destroy(ab,asb)

		end
		
end


function plr_bullet_hit()

	for b in all(bullets) do
			
			local hit_ast,a = collided(asteroids, b, 10)
			local hit_alien, s = collided(alien, b, 10)
			
			if hit_alien then
				on_bullet_hit(s)
   	add_points(plr,s)
   	destroy(bullets,b)	
   	sfx(2)
			end
			
			
			if hit_ast then
				add_points(plr,a)
    on_bullet_hit(a)
    destroy(bullets,b)
    		sfx(1)

    
    	if(a.sprite == 102) then
							create_more(4,asteroids,a,106)
					end
						
   break
    
			end
			
	end
	
end


function on_hit()

	asteroid_hit_plr()
	alien_hit_plr()
	bullet_hit_plr()
	plr_bullet_hit()
	
end



