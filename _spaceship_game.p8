-- asteroids --
	
function asteroid_init()

	asteroids={}
	creation_time=0

end

function asteroid_update()

	asteroid_creation()
	asteroid_movement()

end

function asteroid_creation()

	creation_time+=1
	
	local sp = flr(rnd({64,96,102}))
	local direction = flr(rnd(6))
	 
	-- lookup table for positions 
	local start_directions = {
	
	 [0] = {x=0, y=0},
  [1] = {x=125, y=0},
  [2] = {x=0, y=125},
  [3] = {x=125, y=125},
  [4] = {x=64, y=0},
  [5] = {x=64, y=125}
	
	}
	
	-- set x_value and y_value:
	local x_value = start_directions[direction].x
	local y_value = start_directions[direction].y
	
	
	if creation_time >= 60 then
	
		add(asteroids, {
		x=x_value,  
		y=y_value, 
		speed = 0.7,
		sprite = sp,
		first_sprite = sp,
		exploded = false,
		animation_timer = 0,
		points = 50,
		dir = direction
		
		})
	
		creation_time=0 -- reset
		
	end

end

function asteroid_movement()

	for a in all(asteroids) do
			
			-- direction
			if a.dir == 0 then
				a.y+=a.speed 
				a.x+=a.speed 
			end
					
			if a.dir == 1 then
				a.y+=a.speed 
				a.x-=a.speed 
			end	
			
		if a.dir == 2 then
			a.y -= a.speed -- from bottom-left
			a.x += a.speed
		end

		if a.dir == 3 then
			a.y -= a.speed -- from bottom-right
			a.x -= a.speed
		end	
			
			if a.dir == 4 then
				a.y+=a.speed
			end
			
			if a.dir == 5 then
				a.y-=a.speed
			end
					
	end
			
	
end


