-- general functions --

-- delete when ouutbounds 
function outbound(list)
	
	for i = #list, 1, -1 do
	
		local item = list[i]
		
		if item.x > 128 or item.y > 128 or item.x < 0 or item.y < 0
			then
				destroy(list,item)
		end
		
	end
	
end

-- animates sprite explotion 
function on_explotion(list,timer)

	-- for every item start timer
	for item in all(list) do
	
		item.animation_timer +=1
				
		if item.exploded then 		 
		 item.sprite+=2
	 end
	 
		if item.exploded and item.animation_timer >= timer
			then
  	destroy(list, item)
		end

	end
	
end

-- deletes obj from table
function destroy(list,item)
		del(list,item)
end

-- if two objects are close enough
function collided(list,obj,range)
	
	for item in all(list) do
		
		if(abs(item.x - obj.x) < range and abs(item.y - obj.y) < range)
			then
				return true, item	
		end
		
	end
	
end

-- creates multiple of an object
function create_more(amount,list,item,sprite_value)
	
	for i=1,amount do
		add(list,{
		x	= abs(item.x+rnd(20)),
		y	=	abs(item.y+rnd(20)),
		speed = item.speed,
		sprite = sprite_value,
		first_sprite = sprite_value,
		exploded = false,
		animation_timer = 0,
		points = 10
		})
	end
	
end


-- plr bullet hits something
function on_bullet_hit(item)

	item.exploded = true 
	item.animation_timer = 0
	item.sprite = item.first_sprite
	
end

-- how the object behaves 
function obj_direction(list,item)
	
	for item in all(list) do
	
		item.x += item.spdx -- x speed
		item.y += item.spdy -- y speed
	
	end
	
end

-- draws bullets 
function bullet_draw(item,list)
 
	for item in all(list) do
	
		spr(item.sprite,item.x,item.y)
	
	end	
	
end

-- animates an object
function animation(list,item,timer)

	for item in all(list) do
	
		item.animation_timer +=1
		
		if item.animation_timer % 2 == 0 then		
			item.sprite+=2
		end
			
		if item.animation_timer >= timer
			then
			item.sprite = item.first_sprite
			item.animation_timer = 0
		end
		
	end -- for 

end


-- draws enemies
function enemy_draw(list)

	for item in all(list) do
			spr(item.sprite,item.x,item.y,2,2)
	end
	
end



function all_outbounds()

	local lists = {bullets,asteroids,alien,ab}
	
	for list in all(lists) do
		outbound(list)
	end
	
end

function general_animations()

	on_explotion(asteroids,2)
	on_explotion(alien,6)
	animation(alien,s, 8)

end

