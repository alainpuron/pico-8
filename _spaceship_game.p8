function move(plr)

  -- moves toward the direction
  if btn(â¬†ï¸) then
  	plr.vx += cos(plr.angle) * 2
  	plr.vy += sin(plr.angle) * 2
  end
   
		--apply friction every frame
		plr.vx *= 0.90
		plr.vy *= 0.90
  
		--ship position  
  plr.x += plr.vx 
