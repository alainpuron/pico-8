function alien_init()

	alien={}
	alien_creation_timer = 0
	
end

function alien_update()

			alien_creation(plr) -- create
			alien_movement(alien) -- move 
			
			obj_direction(ab,asb) -- bullet aims
			ab_shoot(s) -- shooting
			
			
end

-- creates the alien --
function alien_creation(plr)

	alien_creation_timer+=1
	
	if rnd(100) < 1 and plr.points > 0 and #alien <= 3 and alien_creation_timer >= 60 then 
				
			local direction = flr(rnd(4))
			
			-- lookup table for positions 
			local start_directions = {
			 [0] = {x=120, y=60},
		  [1] = {x=120, y=75},
		  [2] = {x=0, y=75},
		  [3] = {x=0, y=65}		
			}	
			
				-- set x_value and y_value:
			local x_value = start_directions[direction].x
			local y_value = start_directions[direction].y
			
			add(alien,{	
			x	=	x_value,
			y	=	y_value,
			sprite = 8,
			first_sprite = 8,
			animation_timer = 0,
			speed = 1,
			dir = direction,
			exploded = false,
			points = 100

	})
	
	alien_creation_timer = 0
	end
	
end

function alien_movement(alien)

  for s in all(alien) do
  	
  		if s.dir == 0 or s.dir == 1 then
  				s.x -=s.speed
  		end
  		
  		if s.dir == 2 or s.dir == 3 then
  				s.x +=s.speed
  		end
  		
  end
  
end


-- alien ship shooting --

function alien_bullet()
 -- alien bullet
	ab = {}
	ab_creation_time = 0
	
end

function ab_shoot(s)

	
		if time() % 2 < 0.01  then

			for s in all(alien) do

				local dx = plr.x - s.x
				local dy = plr.y - s.y
				local dist = sqrt(dx*dx + dy*dy)
			
					add(ab, {
					x= s.x-3 ,  
					y= s.y-7 , 
					dmg = 3, 
					sprite = 1,
					
					-- shoots toward the player
					spdx = dx / dist * 1,
					spdy = dy / dist * 1
				
				})

			end
		
		end	
	
end


