function is_tile(tile_type,x,y)

	 local tx, ty = get_tile_pos(x,y)

		local tile =	map_data[ty][tx]
		
		
  
		-- tile has flag
		has_flag = fget(tile,tile_type)
	
		print(has_flag)
		print(tile)
		-- if true
		return has_flag

end



