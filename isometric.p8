function animation(plr)
	
		plr.animation_timer +=1
		
		if plr.animation_timer % 2 == 0 then		
			plr.sprite+=2
		end
			
		if plr.animation_timer >= 6
			then
			plr.sprite = plr.first_sprite
			plr.animation_timer = 0
		end
		
end


