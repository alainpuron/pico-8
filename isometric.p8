  {8,8,8,8,8,8,8,8}
}

	
	tree_layer =	{}
	tree_layer = create_layer(map_data)


end -- function


function create_layer(base)

    local layer = {}
    for y=1, #base do
        layer[y] = {}
        for x=1, #base[y] do
            layer[y][x] = nil
        end
    end
    return layer
end


function iso_coords(x, y)

  local sx = (x - y) * 16 + 64  -- 16 = half width of 32px tile
  local sy = (x + y) * 8        -- 8 = half height of 16px tile
  return sx, sy
  
end

function draw_map(tiles)

 -- gets the height(y) and width(x)
	for y=1,#map_data do
	
    for x=1,#map_data[y] do
    
    		-- what tile is it
      local tile = map_data[y][x]
      local sx, sy = iso_coords(x, y)
      spr(tile, sx, sy, 4, 4)
  
       -- draw overlay if true
      local tree_tile = tree_layer[y][x]
            
      if tree_tile then
							spr(tree_tile, sx, sy -16, 4, 4)
						end
						
      
    end -- for loop(2)
    
  end -- for loop(1)
  
end

function generate_overlay(tile,replacement,layer,chance)

	for y=1, #map_data do
		for x=1,#map_data[y] do
			
			-- if is a grass tile
			if map_data[y][x] == tile then
					
					-- rand pick a grass tile
					-- and over that tile
					-- place a tree 
					if flr(rnd(10)) < chance then
						layer[y][x] = replacement
						else
							layer[y][x] = nil
					end
					
			end
			
		end -- for(2)
	end -- for(1)
	
end
