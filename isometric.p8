-- map gen --

function map_tiles()
	
	tiles ={
	grass = 8,
	water = 4,
	half = 12,
	tree = 128,
	soil = 132 
	}
	
	-- tile type
	tree=0 --flag
		
end


function map_layout()

	map_tiles()
	
	map_data = {
  {8,128,128,8,8,8,8,8},
  {8,128,128,8,8,8,8,8},
  {8,128,128,8,8,8,8,8},
  {8,128,128,8,8,8,8,8},
  {8,8,8,8,8,8,8,8},
  {8,4,4,4,8,8,8,8},
  {8,4,4,4,8,8,8,8},
  {8,8,8,8,8,8,8,8},
  {8,128,128,8,8,8,8,8},
  {8,8,8,8,8,8,8,8}
}

end


function iso_coords(x, y)

  local sx = (x - y) * 16 + 64  -- 16 = half width of 32px tile
  local sy = (x + y) * 8        -- 8 = half height of 16px tile
  return sx, sy
  
end

function draw_map(tiles)

 -- gets the height(y) and width(x)
	for y=1,#map_data do
	
    for x=1,#map_data[y] do
    
    		-- what tile is it
      local tile = map_data[y][x]
      
      if tile != 0 then
      
      		-- calc the location to go
        local sx, sy = iso_coords(x, y)
        
       
  					 -- draw tiles 
  					 
       	if tile == 8 then
       		spr_id = tiles.grass
       	end
       	
       	if tile == 4 then
       		spr_id = tiles.water
       	end
       	
       	if tile == 12 then
       		spr_id = tiles.half
       	end
       	
       	if tile == 128 then
       		spr_id = tiles.tree
       		sy-=16 -- adjust 
       	end
       	
       	if tile == 132 then
       		spr_id = tiles.soil
       	end
       	
        
        spr(spr_id, sx, sy, 4, 4)
      end
      
    end
    
  end
  
end


