 local spawned_fish = 0
	
	
	
			
		
			local sp = rnd({7,8,9})
			
			for x=0,15 do
			
				for y =0,15 do
				
					if rnd(5)<.5 then
						mset(x,y,sp)
						spawned_fish+=1
					end
					
				end
			end
			
			
			
			
			
	
	
	
end

